{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "standard-with-typescript",
	"parser": "@typescript-eslint/parser",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"prettier/prettier": "error"
	}
}
